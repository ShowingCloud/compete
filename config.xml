<?xml version='1.0' encoding='utf-8'?>
<widget id="cn.robodou.competition" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Robodou</name>
    <description>
          The Robodou complete app.
        </description>
    <author email="service@robodou.cn" href="http://intro.domelab.com">
            Robodou Team
        </author>
    <access origin="*" />
    <allow-navigation href="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="Orientation" value="landscape" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="cordova-plugin-device" spec="~1.1.2" />
    <plugin name="cordova-plugin-battery-status" spec="~1.1.2" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.1" />
    <plugin name="cordova-plugin-camera" spec="~2.2.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-sqlite-2" spec="~1.0.3" />
    <plugin name="cordova-plugin-video-editor" spec="~1.0.9" />
    <plugin name="cordova-plugin-ble-central" spec="https://github.com/don/cordova-plugin-ble-central" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~6.0.4" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.1" />
    <plugin name="cordova-plugin-file" spec="~4.2.0" />
    <plugin name="cordova-plugin-media" spec="~2.3.0" />
    <plugin name="cordova-plugin-media-capture" spec="~1.3.0" />
    <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2" />
    <plugin name="cordova-plugin-compat" spec="~1.1.0" />
    <plugin name="cordova-plugin-secure-storage" spec="~2.6.3" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.3" />
    <plugin name="cordova-plugin-console" spec="~1.0.5" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.1" />
    <chcp>
      <config-file url="http://192.168.1.112:31284/chcp.json" />
      <local-development enabled="true" />
    </chcp>
</widget>
